{
	"providers": [
		"classpath:${jboss.home.dir}/providers/*"
	],
	"admin": {
		"organization": "master"
	},
	"eventsStore": {
		"provider": "jpa",
		"jpa": {
			"exclude-events": []
		}
	},
	"organization": {
		"provider": "jpa"
	},
	"invoice": {
		"provider": "jpa"
	},
	"invoiceCache": {
		"default": {
			"enabled": true
		}
	},
	"theme": {
		"staticMaxAge": 2592000,
		"cacheTemplates": true,
		"cacheThemes": true,
		"folder": {
			"dir": "${jboss.home.dir}/themes"
		}
	},
	"scheduled": {
		"interval": 900
	},
	"connectionsHttpClient": {
		"default": {
			
		}
	},
	"connectionsJpa": {
		"default": {
			"dataSource": "java:jboss/datasources/OpenfactDS",
			"databaseSchema": "update"
		}
	},
	"organizationCache": {
		"default": {
			"enabled": true
		}
	},
	"connectionsInfinispan": {
		"provider": "default",
		"default": {
			"cacheContainer": "java:comp/env/infinispan/Openfact"
		}
	}
}