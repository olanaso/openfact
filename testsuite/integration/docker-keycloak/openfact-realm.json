  "accessCodeLifespanLogin": 1800,
  "enabled": true,
  "sslRequired": "external",
  "registrationAllowed": false,
  "registrationEmailAsUsername": false,
  "rememberMe": false,
  "verifyEmail": false,
  "resetPasswordAllowed": false,
  "editUsernameAllowed": false,
  "bruteForceProtected": false,
  "maxFailureWaitSeconds": 900,
  "minimumQuickLoginWaitSeconds": 60,
  "waitIncrementSeconds": 60,
  "quickLoginCheckMilliSeconds": 1000,
  "maxDeltaTimeSeconds": 43200,
  "failureFactor": 30,
  "users": [
    {
      "username": "user1",
      "enabled": true,
      "credentials": [
        {
          "type": "password",
          "value": "password"
        }
      ],
      "realmRoles": [
        "admin"
      ],
      "attributes": {
        "organization": "master"
      }
    },
    {
      "username": "user2",
      "enabled": true,
      "credentials": [
        {
          "type": "password",
          "value": "password"
        }
      ],
      "realmRoles": [
        "admin-organization"
      ],
      "attributes": {
        "organization": "demo"
      }
    },
    {
      "username": "user3",
      "enabled": true,
      "credentials": [
        {
          "type": "password",
          "value": "password"
        }
      ],
      "realmRoles": [
        "organization-user"
      ],
      "attributes": {
        "organization": "demo"
      }
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "admin",
        "composite": true,
        "composites": {
          "client": {
            "openfact": [
              "admin",
              "create-organization", "organization-admin",
              "manage-organization", "manage-invoices", "manage-credit-notes", "manage-debit-notes", "manage-events",
              "view-organization", "view-invoices", "view-credit-notes", "view-debit-notes", "view-events"
            ]
          }
        },
        "clientRole": false,
        "containerId": "openfact"
      },
      {
        "name": "admin-organization",
        "composite": true,
        "composites": {
          "client": {
            "openfact": [
              "organization-admin",
              "manage-organization", "manage-invoices", "manage-credit-notes", "manage-debit-notes", "manage-events",
              "view-organization", "view-invoices", "view-credit-notes", "view-debit-notes", "view-events"
            ]
          }
        },
        "clientRole": false,
        "containerId": "openfact"
      },
      {
        "name": "organization-user",
        "composite": true,
        "composites": {
          "client": {
            "openfact": [
              "manage-invoices", "manage-credit-notes", "manage-debit-notes", "manage-events",
              "view-organization", "view-invoices", "view-credit-notes", "view-debit-notes", "view-events"
            ]
          }
        },
        "clientRole": false,
        "containerId": "openfact"
      }
    ],
    "client": {
      "openfact": [
        { "name": "admin" },
        { "name": "create-organization" }, { "name": "organization-admin" },
        { "name": "manage-organization" }, { "name": "manage-invoices" }, { "name": "manage-credit-notes" }, { "name": "manage-debit-notes" }, { "name": "manage-events" },
